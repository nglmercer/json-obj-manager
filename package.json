{
  "name": "json-obj-manager",
  "version": "1.3.8",
  "description": "Agnostic JSON data manager for Node.js and the browser.",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "keywords": [
    "json",
    "data",
    "storage",
    "node",
    "adapter",
    "browser",
    "localstorage"
  ],
  "author": "memelser",
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/nglmercer/json-obj-manager",
  "bugs": {
    "url": "https://github.com/nglmercer/json-obj-manager/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/esm/node.d.ts",
        "default": "./dist/esm/node.js"
      },
      "require": {
        "types": "./dist/esm/node.d.ts",
        "default": "./dist/cjs/node.js"
      }
    },
    "./browser": {
      "import": {
        "types": "./dist/esm/browser.d.ts",
        "default": "./dist/esm/browser.js"
      },
      "require": {
        "types": "./dist/esm/browser.d.ts",
        "default": "./dist/cjs/browser.js"
      }
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "rimraf": "^6.0.1",
    "typescript": "^5.0.0"
  }
}
