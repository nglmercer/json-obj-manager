{
  "name": "json-obj-manager",
  "version": "1.4.0",
  "description": "Agnostic JSON data manager for Node.js and the browser.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "json",
    "data", "storage",
    "node", "adapter",
    "browser", "localstorage"
  ],
  "author": "memelser",
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/nglmercer/json-obj-manager",
  "bugs": {
    "url": "https://github.com/nglmercer/json-obj-manager/issues"
  },
  "scripts": {
    "build": "npm run build:tsc && npm run build:microbundle",
    "build:tsc": "tsc",
    "build:microbundle": "microbundle --tsconfig tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/node.js",
      "require": "./dist/node.cjs",
      "types": "./dist/node.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs",
      "types": "./dist/browser.d.ts"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0",
    "microbundle": "^0.15.1"
  }
}